<IfModule mod_negotiation.c>
    Options -MultiViews
</IfModule>
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    <!-- RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f -->
    RewriteRule ^(.*)$ index.php/$1 [QSA,PT,L]
    RewriteRule ^destinations/(.*)$ destinations.php?destId=$1 [NC,L]
</IfModule>